{
  "name": "share-pro",
  "version": "1.15.1",
  "description": "share your notes from siyuan-note with one click",
  "type": "module",
  "repository": "terwer/share-pro",
  "homepage": "https://github.com/terwerinc/siyuan-plugin-share-pro",
  "author": "terwer",
  "license": "MIT",
  "scripts": {
    "makeLink": "python scripts/make_dev_link.py",
