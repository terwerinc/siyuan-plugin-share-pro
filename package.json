    "@types/minimist": "1.2.5",
    "@types/node": "^22.19.2",
    "fast-glob": "^3.3.3",
    "jsdom": "^26.1.0",
    "minimist": "^1.2.8",
    "rollup-plugin-livereload": "^2.0.5",
    "siyuan": "^1.1.6",
    "stylus": "^0.64.0",
    "svelte": "^4.2.20",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node-polyfills": "^0.24.0",
